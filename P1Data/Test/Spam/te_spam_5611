function validate_required(id,alerttxt)
{
with (id)
  {
  if (value=3D=3Dnull||value=3D=3D"")
    {
    alert(alerttxt);return false;
    }
  else
    {
    return true;
    }
  }
}

function validate_form(thisform)
{
with (thisform)
  {
    if (validate_required(salutation,"Please enter your Title")=3D=3Dfalse)
  {salutation.focus();return false;}
  if (validate_required(firstname,"Please enter your Forename")=3D=3Dfalse)
  {firstname.focus();return false;}
  if (validate_required(lastname,"Please enter your Surname")=3D=3Dfalse)
  {lastname.focus();return false;}
    if (validate_required(emailaddress1,"Please enter your Email =
Address")=3D=3Dfalse)
  {emailaddress1.focus();return false;}
  if (validate_required(birthdate,"Please enter your Date of =
Birth")=3D=3Dfalse)
  {birthdate.focus();return false;}
    if (validate_required(gendercode,"Please enter your =
Gender")=3D=3Dfalse)
  {gendercode.focus();return false;}
  if (validate_required(address1,"Please enter your Address")=3D=3Dfalse)
  {address1.focus();return false;}
    if (validate_required(address1_city,"Please enter your =
Town")=3D=3Dfalse)
  {address1_city.focus();return false;}
    if (validate_required(address1_country,"Please enter the Country that =
you live in")=3D=3Dfalse)
  {address1_country.focus();return false;}

}
}
</script>

